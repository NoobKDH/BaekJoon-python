T = int(input())
for i in range(T):
    N = int(input())
    for j in range(N):
        school, soju = map(str, input().split())
        if j ==0:
            resch = school
            reso = int(soju)
        else:
            if reso < int(soju):
                resch = school
                reso = int(soju)
    print(resch)
